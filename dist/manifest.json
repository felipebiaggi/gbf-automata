{
  "manifest_version": 3,
  "name": "Granblue Fantasy: Request Analyzer",
  "version": "1.0",
  "background": {
    "service_worker": "worker.bundle.js"
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["content.bundle.js"],
      "matches": ["https://game.granbluefantasy.jp/*"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "contextMenus",
    "background",
    "offscreen"
  ],
  "host_permissions": ["https://game.granbluefantasy.jp/*"]
}
